{
  "name": "react-application",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "node_modules/.bin/webpack --mode development",
    "build:prod": "node_modules/.bin/webpack --config=webpack.prod.config.js --mode production",
    "dev-client": "webpack-dev-server --open",
    "dev-server": "cross-env NODE_ENV=development node server/index.js",
    "test": "jest",
    "e2e": "cypress open"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lubov15911/React-application.git"
